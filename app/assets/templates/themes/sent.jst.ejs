<div class="saved-templates col-xs-12">
  <h3>Successfully Sent Emails</h3><br>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Recipient</th>
        <th>Title</th>
        <th>Date Sent</th>
      </tr>
    </thead>
    <tbody>
  <% themes.forEach(function(theme){ %>
    <% if (theme.escape('sent') === 'true') { %>  
      <% var theme_date = new Date(Date.parse(theme.escape('updated_at'))) %>        
    <tr>  
        <td><%= theme.escape('recipient_email') %></td>
        <td><%= theme.escape('title') %></td>
        <td><%= theme_date.toUTCString().slice(0, -4) %></td>
    </tr>
    <% } %> 
  <% }); %> 
    </tbody>
  </table>  
</div>