<div class="saved-templates col-xs-12" style="padding-top: 45px">   
  <a href="#/" class="btn btn-default" style="margin-bottom:15px;">Back</a>   
  <div class="box box-success">
    <div class="box-header">
      <h3 class="box-title">Sent History</h3>
    </div>
    <div class="box-body table-responsive no-padding">
      <table class="table table-hover">
        <tbody>
          <tr>
            <th>Recipient</th>
            <th>Subject</th>
            <th>Date Sent</th>
          </tr>
          <% themes.forEach(function(theme){ %>
            <% if (theme.escape('sent') === 'true') { %> 
              <% var theme_date = new Date(Date.parse(theme.escape('updated_at'))) %>            
                <tr>
                  <td><%= theme.escape('recipient_email') %></td>
                  <td><a href="#/themes/<%= theme.escape('id') %>"><%= theme.escape('title') %></a></td>      
                  <td><%= theme_date.toUTCString().slice(0, -4) %></td>
                </tr>              
            <% } %>  
          <% }); %>        
        </tbody>
      </table>
    </div>
  </div>
</div>